Algoritmo Division
	Definir num1,num2 Como Entero;
	Escribir "Ingrese el dividendo: ";
	Leer num1;
	Escribir "Ingrese el divisor: ";
	Leer num2;
	Si num1 MOD num2 == 0 o  num2 MOD num1 == 0;
		Escribir "Su divixion es exacta" ;
	SiNo;
		Escribir "Su division non es exacta";
	FinSi
	
FinAlgoritmo
